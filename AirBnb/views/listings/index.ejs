<% layout("/layouts/boilerplate") -%>
<body>
   
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
        <% for(let listings of allListings){ %>
            <div class="card col" style="width: 18rem;">
                <img src="<%= listings.image %>" class="card-img-top" alt="listing-image">
                <div class="card-body">
                  <h5 class="card-title"> <%= listings.title %></h5>
                  <p class="card-text"> <%= listings.description %></p>
                  <a href="/listings/<%= listings._id %>" class="btn btn-primary">Checkout</a>
                </div>
              </div>
            <%}%>
    </div>
    
        
</body>